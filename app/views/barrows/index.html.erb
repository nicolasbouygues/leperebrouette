<p id="notice"><%= notice %></p>

<h1>Barrows</h1>


<div class="cards">
  <% @barrows.each do |barrow| %>
     <div class="container">
        <div class="card-trip">
          <% if barrow.photo.nil? %>
          <% elsif barrow.photo.attached? %>
            <%= link_to barrow_path(barrow) do %>
             <%= cl_image_tag barrow.photo.key %>
             <% end %>
          <% end %>
            <div class="card-trip-infos">
            <div>
              <h2><%= barrow.name %> </h2>
              <p> <%= barrow.category %> </p>
              <p> <%= barrow.town %> </p>
              <h2 class="card-trip-pricing">Â£ <%= barrow.price %> </h2>
            </div>
        </div>
    </div>

    </div>
  <% end %>
</div>

